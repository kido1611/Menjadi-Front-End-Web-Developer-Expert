{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base",
        "plugin:codeceptjs/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "import/no-extraneous-dependencies": 0,
        "no-prototype-builtins": "off",
        "consistent-return": "off",
        "import/prefer-default-export": "off"
    }
}
